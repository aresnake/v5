import bpy

def classify_operator(op: str):
    if not op or not isinstance(op, str):
        return "unknown"

    # ğŸ’¡ VÃ©rifie si câ€™est un opÃ©rateur bpy.ops
    parts = op.split(".")
    if len(parts) == 2:
        category, command = parts
        if hasattr(bpy.ops, category) and hasattr(getattr(bpy.ops, category), command):
            return "ops"

    # ğŸ’¡ VÃ©rifie si câ€™est une propriÃ©tÃ© bpy.context
    if hasattr(bpy.context, op.split(".")[0]):
        return "context"

    return "unknown"
